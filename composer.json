{
    "name": "ringlesoft/jasmin-client",
    "description": "A laravel package for interacting with Jasmin SMS Gateway",
    "type": "laravel-plugin",
    "homepage": "https://ringlesoft.com/packages/jasmin-client",
    "license": "MIT",
    "keywords": [
        "ringlesoft",
        "jasmin",
        "jasmin sms gateway",
        "sms",
        "smpp",
        "sms gateway",
        "laravel"
    ],
    "autoload": {
        "psr-4": {
            "RingleSoft\\JasminClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RingleSoft\\JasminClient\\": "src/",
            "RingleSoft\\JasminClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Ringle",
            "email": "ringunger@gmail.com",
            "homepage": "https://ringlesoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.23",
        "illuminate/collections": "^10.0|^11.23",
        "illuminate/http": "^10.0|^11.23",
        "illuminate/validation": "^10.0|^11.23",
        "glushkovds/php-smpp": "^0.7.1"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^11.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RingleSoft\\JasminClient\\JasminClientServiceProvider"
            ],
            "aliases": {
                "LaravelSelectable": "Ringlesoft\\JasminClient\\Facades\\JasminClient"
            },
            "dont-discover": [
            ]
        }
    },
    "branches": {
        "stable": {
            "name": "master"
        }
    },
    "version": "1.0.0",
    "minimum-stability": "dev",
    "prefer-stable": true
}
